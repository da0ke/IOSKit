# Uncomment the next line to define a global platform for your project
platform :ios, '8.0'

target 'IOSKit' do

  use_frameworks!

# 网络框架
# https://github.com/Alamofire/Alamofire
pod 'Alamofire', '4.7.3'

# json框架
# https://github.com/SwiftyJSON/SwiftyJSON
pod 'SwiftyJSON'

# 加密算法，比如md5
# https://github.com/krzyzanowskim/CryptoSwift
pod 'CryptoSwift'


end
